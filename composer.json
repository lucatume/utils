{
  "name": "lucatume/utils",
  "description": "Utility functions.",
  "version": "0.1.0",
  "require": {
    "php": ">=5.6",
    "ext-iconv": "*",
    "ext-json": "*",
    "zordius/lightncandy": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "files": [
      "src/path.php",
      "src/string.php",
      "src/tests.php"
    ],
    "psr-4": {
      "lucatume\\Utils\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/_boostrap.php"
    ]
  }
}
